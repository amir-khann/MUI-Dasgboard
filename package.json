{
    "name": "material-dashboard-pro-react",
    "version": "1.10.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && gulp licenses",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "deploy": "npm run build",
        "lint": "eslint src",
        "format": "prettier --write \"src/**/*.{js,css,scss,html}\"",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "compile-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.css",
        "minify-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.css --source-map true"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@material-ui/pickers": "^3.3.10",
        "@mui/material": "^5.4.1",
        "@mui/x-data-grid": "^5.5.1",
        "axios": "^0.25.0",
        "chartist": "0.10.1",
        "classnames": "2.3.1",
        "formik": "^2.2.9",
        "lodash": "^4.17.21",
        "match-sorter": "6.3.0",
        "material-ui-confirm": "^2.1.3",
        "material-ui-datatables": "^0.18.2",
        "moment": "2.29.1",
        "mui-datatables": "^4.0.0",
        "node-sass": "^6.0.1",
        "nouislider": "15.0.0",
        "perfect-scrollbar": "1.5.1",
        "react": "17.0.2",
        "react-awesome-query-builder": "^4.10.0",
        "react-big-calendar": "0.33.2",
        "react-bootstrap-sweetalert": "5.2.0",
        "react-chartist": "0.14.4",
        "react-datetime": "3.0.4",
        "react-dom": "17.0.2",
        "react-jvectormap": "0.0.16",
        "react-query": "^3.31.0",
        "react-redux": "^7.2.6",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.3",
        "react-swipeable-views": "0.13.9",
        "react-table": "7.7.0",
        "react-tagsinput": "3.19.0",
        "react-text-mask": "^5.4.3",
        "redux": "^4.1.2",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "redux-thunk": "^2.4.0",
        "reselect": "^4.1.4",
        "yup": "^0.32.11"
    },
    "optionalDependencies": {
        "@babel/core": "7.14.0",
        "typescript": "4.2.4"
    },
    "devDependencies": {
        "gulp": "4.0.2",
        "gulp-append-prepend": "1.0.9",
        "prettier": "^1.18.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": []
    }
}
